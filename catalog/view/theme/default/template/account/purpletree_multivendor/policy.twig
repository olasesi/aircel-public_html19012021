{{ header }}
<div class="pts-container">
	<ul class="pts-breadcrumb">
		{% for breadcrumb in breadcrumbs %}
		<li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
		{% endfor %}
	</ul>
	<div class="pts-row d-flex">
		<div id="pts-content" class="pts-col-sm-12">
			{{ content_top }}
				<h1>{{ text_policy }}</h1>
				{{ store_policy }}
			{{ content_bottom }}
		</div>
	</div>
</div>
{{ footer }}